{
  "name": "angular-universal-minimal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "dev:ssr": "ng run angular-universal-minimal:serve-ssr",
    "serve:ssr": "node dist/angular-universal-minimal/server/main.js",
    "build:ssr": "ng build && ng run angular-universal-minimal:server",
    "prerender": "node ./fetch-routes.js && ng run angular-universal-minimal:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/platform-server": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@magnolia/angular-editor": "^1.3.1",
    "@nguniversal/express-engine": "^14.2.0",
    "domino": "^2.1.6",
    "express": "^4.18.2",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.8",
    "@angular/cli": "~14.2.8",
